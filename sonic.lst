0001E8F8                            setspeed1:
0001E8F8 0828 0006 0022             	btst	#6,status(a0)			; Is the character underwater?
0001E8FE 6700                       	beq.s	setspeed2				; If not, branch
0001E900 5400                       	addq.b	#2,d0				; Quickly add 2 to d0
0001E902                            setspeed2:
0001E902 4A38 FE19                  	tst.b	(Super_Sonic_flag).w		; Is the character Super?
0001E906 6700                       	beq.s	setspeed3				; If not, branch
