0001E8EE                            ApplySpeedSettings:
0001E8EE 7000                       	moveq	#0,d0				; Quickly clear d0
0001E8F0 4A28 0034                  	tst.b	speedshoes_time(a0)		; Does character have speedshoes?
0001E8F4 6700                       	beq.s	setspeed1				; If not, branch
