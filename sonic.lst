00000000                            ; 2-arg instruction that's self-patching to remove 0-offset optimization
00000000                            insn2op	 macro oper,x,y
00000000                            	  if (substr("x",0,2)<>"0(") && (substr("x",0,3)<>"ID(")
00000000                            		  if (substr("y",0,2)<>"0(") && (substr("y",0,3)<>"ID(")
00000000                            			!oper	x,y
00000000                            		  else
00000000                            			!oper	x,1+y
00000000                            			!org	*-1
00000000                            			!dc.b	0
00000000                            		  endif
00000000                            	  else
00000000                            		if substr("y",0,1)<>"D"
00000000                            		  if (substr("y",0,2)<>"0(") && (substr("y",0,3)<>"ID(")
00000000                            start:
00000000                            			!oper	1+x,y
00000000                            end:
00000000                            			!org	start+3
00000000                            			!dc.b	0
00000000                            			!org	end
00000000                            		  else
00000000                            			!oper	1+x,1+y
00000000                            			!org	*-3
00000000                            			!dc.b	0
00000000                            			!org	*+1
00000000                            			!dc.b	0
00000000                            		  endif
00000000                            		else
00000000                            			!oper	1+x,y
00000000                            			!org	*-1
00000000                            			!dc.b	0
00000000                            		endif
00000000                            	  endif
