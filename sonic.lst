0001E90C D000                       	add.b	d0,d0				; And again
0001E90E D000                       	add.b	d0,d0				; And again
0001E910 43FB 0000                  	lea	Speedsettings(pc,d0.w),a1	; Load correct speed settings into a1
0001E914 5489                       	addq.l	#2,a1				; Increment a1 by 2 quickly
